{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "eqeqeq": "error",
      "no-unused-vars": ["error", { "ignoreRestSiblings": true }]
    }
}
